{
  "name": "2024-github-base",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "deploy": "yarn build && yarn lint && cp vercel.json dist/vercel.json &&  gh-pages --dotfiles -d dist -b main --repo https://github.com/joephela/joephela.github.io.git",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "npx prettier --write '**/*.tsx'",
    "serve": "vite",
    "preinstall": "node -e 'if(parseInt(process.versions.node.split(\".\")[0]) < 20) { console.error(\"Node version 20 or higher is required.\"); process.exit(1); }'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tanstack/react-query": "^5.90.16",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "gh-pages": "^6.3.0",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1"
  },
  "packageManager": "yarn@4.10.2"
}
